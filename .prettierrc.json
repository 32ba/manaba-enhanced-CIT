{
  "endOfLine": "lf",
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "es5",
  "semi": false,
  "parser": "typescript",
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "parser": "json"
      }
    },
    {
      "files": "*.html",
      "options": {
        "parser": "html"
      }
    },
    {
      "files": "*.yaml",
      "options": {
        "parser": "yaml"
      }
    }
  ]
}
